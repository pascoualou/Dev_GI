ADD TABLE "magiToken"
  AREA "Data Area"
  DUMP-NAME "magitoken"

ADD FIELD "jSessionId" OF "magiToken" AS character 
  DESCRIPTION "Spring security token"
  FORMAT "x(8)"
  INITIAL ""
  LABEL "JSessionId"
  POSITION 2
  MAX-WIDTH 16
  ORDER 10

ADD FIELD "horodate" OF "magiToken" AS datetime 
  FORMAT "99/99/9999 HH:MM:SS.SSS"
  INITIAL "now"
  LABEL "horodate"
  POSITION 3
  MAX-WIDTH 8
  ORDER 20

ADD FIELD "iUser" OF "magiToken" AS int64 
  FORMAT "->,>>>,>>9"
  INITIAL "0"
  LABEL "N° utilisateur"
  POSITION 4
  MAX-WIDTH 8
  ORDER 30

ADD FIELD "cValeur" OF "magiToken" AS character 
  FORMAT "x(8)"
  INITIAL ""
  LABEL "valeur token"
  POSITION 5
  MAX-WIDTH 16
  ORDER 40

ADD FIELD "cUser" OF "magiToken" AS character 
  FORMAT "x(8)"
  INITIAL ""
  LABEL "userName"
  POSITION 6
  MAX-WIDTH 16
  ORDER 50

ADD FIELD "cRefPrincipale" OF "magiToken" AS character 
  DESCRIPTION "Référence principale NoRefUse"
  FORMAT "x(10)"
  INITIAL ""
  POSITION 8
  MAX-WIDTH 20
  ORDER 60

ADD FIELD "cRefGerance" OF "magiToken" AS character 
  DESCRIPTION "Référence Gérance NoRefGer"
  FORMAT "x(10)"
  INITIAL ""
  POSITION 9
  MAX-WIDTH 20
  ORDER 70

ADD FIELD "cRefCopro" OF "magiToken" AS character 
  DESCRIPTION "Référence Copro"
  FORMAT "x(10)"
  INITIAL ""
  POSITION 10
  MAX-WIDTH 20
  ORDER 80

ADD FIELD "iCodeSociete" OF "magiToken" AS integer 
  DESCRIPTION "Code Société"
  FORMAT ">>9"
  INITIAL "0"
  POSITION 11
  MAX-WIDTH 4
  ORDER 90

ADD FIELD "iGestionnaire" OF "magiToken" AS integer 
  FORMAT "->,>>>,>>9"
  INITIAL "0"
  LABEL "Gestionnaire"
  POSITION 12
  MAX-WIDTH 4
  ORDER 100

ADD FIELD "iCollaborateur" OF "magiToken" AS integer 
  FORMAT "->,>>>,>>9"
  INITIAL "0"
  LABEL "Collaborateur"
  POSITION 13
  MAX-WIDTH 4
  COLUMN-LABEL "Coll"
  ORDER 110

ADD FIELD "lDebug" OF "magiToken" AS logical 
  FORMAT "on/off"
  INITIAL "on"
  LABEL "Debug"
  POSITION 14
  MAX-WIDTH 1
  ORDER 140

ADD FIELD "iTraceLevel" OF "magiToken" AS integer 
  DESCRIPTION "0: trace explicite bdd
1: trace bdd des run on appServer
2: trace 1 + run pro in
8: trace fichier des run on appServer
9: trace 8 + run pro in
"
  FORMAT "9"
  INITIAL "0"
  LABEL "Niveau trace"
  POSITION 15
  MAX-WIDTH 4
  ORDER 150

ADD FIELD "iCodeLangueSession" OF "magiToken" AS integer 
  FORMAT "9"
  INITIAL "0"
  POSITION 16
  MAX-WIDTH 4
  ORDER 120

ADD FIELD "iCodeLangueReference" OF "magiToken" AS integer 
  FORMAT "9"
  INITIAL "0"
  POSITION 17
  MAX-WIDTH 4
  ORDER 130

ADD INDEX "JSessionId" ON "magiToken" 
  AREA "Index Area"
  UNIQUE
  PRIMARY
  INDEX-FIELD "jSessionId" ASCENDING 

ADD INDEX "secondaire" ON "magiToken" 
  AREA "Data Area"
  INDEX-FIELD "horodate" ASCENDING 

.
PSC
cpstream=ISO8859-1
.
0000002842
